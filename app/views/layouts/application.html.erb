<!DOCTYPE html>
<html>
<head>
  <title>Attente dans les urgences</title>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
	<!--[if lt IE 9]>
	<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
	<%= stylesheet_link_tag 'blueprint/screen', :media => 'screen' %>
	<!--[if lt IE 8]>
	<%= stylesheet_link_tag 'blueprint/ie' %>
	<![endif]-->
	<%= stylesheet_link_tag 'default', :media => 'all' %>
</head>
<body>
	<div class="container">
		<header>
			<%= image_tag("logo.png", :alt => "AttenteUrgence.ca", :class=> "round") %>
			<nav class="round">
				<u1>
				<li><%= link_to "Accueil", '#' %></li>
				<li><%= link_to "A propos", '#' %></li>
				</u1>
			</nav>
		</header>				
		<section class="round">
		<%= yield %>	
	</div>
</body>
</html>
